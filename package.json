{
  "name": "angular-t9n",
  "version": "0.3.1",
  "description": "A translation tool for Angular i18n",
  "keywords": [
    "angular",
    "i18n",
    "xliff",
    "xlf",
    "xlf2",
    "translation",
    "t9n"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:ng && npm run build:node",
    "build:ng": "ng build --prod",
    "build:node": "rollup --config rollup.config.js",
    "format": "prettier --write **/*",
    "test": "npm run test:node",
    "test:node": "jest",
    "lint": "ng lint",
    "xi18n:xlf": "ng xi18n -c xlf",
    "xi18n:xlf2": "ng xi18n",
    "xi18n": "npm run xi18n:xlf && npm run xi18n:xlf2",
    "t9n:xlf": "ng run angular-t9n:t9n -c xlf",
    "t9n:xlf2": "ng run angular-t9n:t9n",
    "release": "standard-version --tag-prefix=\"\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyubisation/angular-t9n.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kyubisation/angular-t9n/issues"
  },
  "files": [
    "{builders,schematics}/**/*.{json,js,d.ts}",
    "app/**/*.*",
    "!tsconfig.json"
  ],
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-ws": "^7.2.0",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/websockets": "^7.1.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "js-levenshtein": "^1.1.6",
    "reflect-metadata": "^0.1.13",
    "xmldom": "^0.2.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "^10.0.1",
    "@angular/cli": "~10.0.2",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@angular/localize": "^10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jasmine": "~3.3.16",
    "@types/jasminewd2": "~2.0.8",
    "@types/jest": "^26.0.0",
    "@types/js-levenshtein": "^1.1.0",
    "@types/node": "^10.0.0",
    "@types/supertest": "^2.0.9",
    "@types/ws": "^7.2.5",
    "@types/xmldom": "^0.1.29",
    "@wessberg/rollup-plugin-ts": "^1.2.25",
    "codelyzer": "^6.0.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "26.0.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.17.0",
    "rxjs": "~6.5.5",
    "standard-version": "^8.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.9.6",
    "xlsx": "^0.16.2",
    "zone.js": "~0.10.3"
  },
  "schematics": "./schematics/collection.json",
  "builders": "./builders/builders.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.node.json"
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.module.ts",
      "!**/*.d.ts",
      "!**/index.ts",
      "!**/*.spec.ts"
    ],
    "coverageDirectory": "coverage"
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "lf",
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,scss,json,md,html}": [
      "npx prettier --write",
      "git add"
    ]
  }
}
